datasource db {
    provider = "postgresql"
    url = "postgresql://postgres:aaron@localhost:5432/bagonia_medical"
}

generator client {
    provider = "prisma-client-js"
}

model Inventory {
    id          Int         @id @default(autoincrement())
    name        String
    category    String
    quantity    Int
    price       Float?
    createdAt   DateTime    @default(now())
    updatedAt   DateTime    @updatedAt
}

model Receipts {
    
}